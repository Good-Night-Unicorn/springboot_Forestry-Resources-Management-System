<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.KaifaqiyeDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.KaifaqiyeEntity" id="kaifaqiyeMap">
        <result property="qiyemingcheng" column="qiyemingcheng"/>
        <result property="mima" column="mima"/>
        <result property="fuzeren" column="fuzeren"/>
        <result property="xingbie" column="xingbie"/>
        <result property="shouji" column="shouji"/>
        <result property="touxiang" column="touxiang"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.KaifaqiyeVO" >
		SELECT * FROM kaifaqiye  kaifaqiye         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.KaifaqiyeVO" >
		SELECT  kaifaqiye.* FROM kaifaqiye  kaifaqiye 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.KaifaqiyeView" >

		SELECT  kaifaqiye.* FROM kaifaqiye  kaifaqiye 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.KaifaqiyeView" >
		SELECT * FROM kaifaqiye  kaifaqiye <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	


</mapper>
